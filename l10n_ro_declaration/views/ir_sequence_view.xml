<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="sequence_view" model="ir.ui.view">
        <field name="name">ir.sequence.form</field>
        <field name="model">ir.sequence</field>
        <field name="inherit_id" ref="base.sequence_view" />
        <field name="arch" type="xml">
            <field name="use_date_range" position="after">
                <field name="number_first" />
                <field name="number_last" />
            </field>
            <xpath
                expr="//field[@name='date_range_ids']/tree/field[@name='date_to']"
                position="after"
            >
                <field name="number_first" />
                <field name="number_last" />
            </xpath>
        </field>
    </record>
</odoo>
