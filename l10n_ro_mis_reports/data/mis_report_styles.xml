      <field name="font_weight_inherit" eval="0"/>
      <field name="font_weight">bold</field>
    </record>

    <record id="mis_report_style_l10n_ro_2i" model="mis.report.style">
      <field name="name">l10n_ro 2i</field>
      <field name="font_weight_inherit" eval="0"/>
