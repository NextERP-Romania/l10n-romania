<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="view_move_form_inherited394" model="ir.ui.view">
         <field name="name">account.move.form.inheritd394</field>
         <field name="model">account.move</field>
         <field name="inherit_id" ref="account.view_move_form"/>
         <field name="arch" type="xml">
                <xpath expr="//field[@name='edi_state']" position="after">
                    <field name = "invoice_origin_d394"/>
                </xpath>
            </field>
    </record>


    <record id="view_account_journal_form_inheritedD394" model="ir.ui.view">
         <field name="name">account.journal.form.inheritdD394</field>
         <field name="model">account.journal</field>
         <field name="inherit_id" ref="account.view_account_journal_form"/>
         <field name="arch" type="xml">
                <xpath expr="//group[@name='group_alias_edit']" position="after">
                      <group string="Declaratia 394" attrs="{'invisible': [('type', '!=', 'sale')]}" >
						<field name="sequence_type" />
						<field name="partner_id" />
					</group>
                </xpath>
            </field>
    </record>
</odoo>
